<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>

<GridPane prefHeight="480.0" prefWidth="720.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1">
  <columnConstraints>
    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
  </columnConstraints>
  <rowConstraints>
    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
  </rowConstraints>
  <children>
    <Label text="Radius" />
    <TextField fx:id="radius" GridPane.rowIndex="1" />
    <Separator orientation="VERTICAL" prefHeight="200.0" GridPane.rowIndex="2" />
    <Label text="Bank" GridPane.rowIndex="3" />
    <ComboBox fx:id="bankArea" prefHeight="31.0" prefWidth="724.0" GridPane.hgrow="ALWAYS" GridPane.rowIndex="4" />
    <Separator orientation="VERTICAL" prefHeight="200.0" GridPane.rowIndex="5" />
    <Label text="Tree" GridPane.rowIndex="6" />
    <ComboBox fx:id="treeType" prefHeight="31.0" prefWidth="724.0" GridPane.hgrow="ALWAYS" GridPane.rowIndex="7" />
    <CheckBox fx:id="drop" mnemonicParsing="false" text="Drop?" GridPane.rowIndex="8" />
    <Button fx:id="init" alignment="CENTER" mnemonicParsing="false" prefHeight="31.0" prefWidth="720.0" text="Start" GridPane.hgrow="ALWAYS" GridPane.rowIndex="9" />
  </children>
</GridPane>
